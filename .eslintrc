{
  "extends": ["eslint:recommended", "google"],
  "rules": {
    "no-console": 0,
    "async-await/space-after-async": 2,
    "async-await/space-after-await": 2,
    "require-jsdoc": 0,
    "arrow-parens": 0,
    "max-len": ["error", 100],
    "comma-dangle": 0,
    "object-curly-spacing": "off"
  },
  "parserOptions": {
    "sourceType": "module",
    "ecmaVersion": 8,
    "ecmaFeatures": {
      "jsx": true,
      "experimentalObjectRestSpread": true
    }
  },
  "plugins": ["async-await", "react"],
  "env": {
    "node": true,
    "es6": true,
    "mocha": true
  }
}
